      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.mo_wael.smartrice"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "fab4cf7e-080c-4d2c-a5dd-acd4782d703e"
      }
    },
    "plugins": ["expo-font"]
  }
}
