{
  "expo": {
    "name": "SmartRice Dwelling",
    "slug": "smart_house_feed",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
